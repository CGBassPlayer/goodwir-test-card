{
  "include": [
      "./src/"
  ],
  "compilerOptions": {
      "target": "es5",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "outDir": "./dist/",
      "rootDir": "./src",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true
  }
}